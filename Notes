if let url = URL(string: "http://www.shiny-ideas.tech/hb/hb.html") {
    do {
        let contents = try String(contentsOf: url)
        print(contents)
    } catch {
        print("Cannot access URL")
    }
} else {
    print("Bad URL supplied")
}


